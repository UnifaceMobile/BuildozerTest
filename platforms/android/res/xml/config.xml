<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="" id="com.uniface.client.previewer" ios-CFBundleVersion="" version="1.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <name>Previewer</name>
    <description>This is a scanner app from Uniface for testing and debugging purpose</description>
    <author email="Mobile@uniface.com" href="http://www.uniface.com">Uniface Mobile Team</author>
    <preference name="phonegap-version" value="4.1.2" />
    <preference name="orientation" value="default" />
    <preference name="fullscreen" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="target-device" value="universal" />
    <preference name="prerendered-icon" value="false" />
    <preference name="detect-data-types" value="false" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="deployment-target" value="7" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="BackupWebStorage" value="local" />
    <preference name="android-installLocation" value="auto" />
    <preference name="android-windowSoftInputMode" value="stateVisible|adjustResize" />
    <preference name="SplashScreen" value="splash" />
    <preference name="SplashScreenDelay" value="3000" />
    <access origin="*" subdomains="true" />
    <allow-intent href="*" />
    <allow-navigation href="*" />
    <content src="index.html" />
    <gap:plugin name="org.apache.cordova.statusbar" />
    <icon src="icon.png" />
